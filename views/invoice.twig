<html lang="ar" dir="rtl">
	<head>
		<meta charset="UTF-8"/>
		<title>Invoice #{{id}} / فاتورة #{{ id|arabicDigits }}</title>
		<style>
			html, body {
				margin: 0;
				padding: 0;
			}

			body {
				color: #272C2D;
				font-size: 12pt;
				width: auto;
			}

			.title-container {
				height: 55pt;
			}

			.title-value {
				padding-top: 8pt;
				font-size: 12pt;
			}

			.title-text {
				font-size: 8pt;
			}

			.align-right {
				text-align: end;
			}

			.text-bold {
				font-weight: bold;
			}

			.px-10 {
				padding-left: 10pt;
				padding-right: 10pt;
			}

			.py-5 {
				padding-top: 5pt;
				padding-bottom: 5pt;
			}

			.p-10 {
				padding-left: 10pt;
				padding-right: 10pt;
				padding-top: 10pt;
				padding-bottom: 10pt;
			}

			.table-header {
				font-weight: 600;
				padding-left: 10pt;
				padding-right: 10pt;
				padding-top: 5pt;
				padding-bottom: 5pt;
				text-align: start;
				font-size: 12pt;
			}

			.font-bold {
				font-weight: 700;
			}

			.text-small {
				font-size: 12pt;
			}

			.text-large {
				font-size: 18pt;
			}

			table#invoice {
				border-collapse: collapse;
			}

			table#invoice td {
				font-size: 12pt;
				border: 2pt solid #ECF0F1;
			}

			table#invoice .border-none {
				border: 0;
			}
		</style>
	</head>

	<body>
		<div style="height: 90%">
			<table style="padding: 5pt 5pt; width: 100%">
				<tbody>
					<tr>
						<td style="padding-bottom: 20pt;">
							<img src="data:image/png;base64," width="150" height="auto"/>
						</td>
					</tr>
						<tr>
							<td class="title-container">
						<div class="title-text">من</div>
							<div class="title-value">تكامل القابضة</div>
						</td>
						<td >
							<div class="title-text">الرقم الضريبي</div>
							<div class="title-value">{{ tax_number}}</div>
						</td>
						<td style="direction: ltr;">
							<div class="title-text">Tax number</div>
							<div class="title-value">{{tax_number}}</div>
						</td>
						<td style="direction: ltr;">
							<div class="title-text">From</div>
							<div class="title-value">Takamol Holding</div>
						</td>
					</tr>
				</tbody>
			</table>
			<hr color="#152e83"/>
			<div style="height: 50pt"></div>
			<table style="padding: 5pt 5pt; width: 100%">
				<tbody>
					<tr>
				<td class="title-container">
							<div class="text-bold text-large">فاتورة</div>
							<div class="text-bold text-large">#{{ id|arabicDigits }}</div>
						</td>
							<td class="title-container" style="direction: ltr;">
							<div class="text-bold text-large">Invoice</div>
							<div class="text-bold text-large">#{{ id }}</div>
						</td>

					</tr>
					<tr>
					<td class="title-container">
							<div class="title-text">الى</div>
							<div class="title-value">{{ company_name_ar }}</div>
						</td>
						<td class="title-container" style="direction: ltr;">
							<div class="title-text">To</div>
							<div class="title-value">{{ company_name_en }}</div>
						</td>
					</tr>
				</tbody>
			</table>
			<div style="height: 40pt"></div>
			<table style="padding: 5pt 5pt; width: 100%">
				<tbody>
					<tr>
						{% if payment_method == 'qiwa' %}
								<td class="title-container">
								<div class="title-text">طريقة الدفع</div>
								<div class="title-value">فيزا و مدى</div>
							</td>
							<td class="title-container">
								<div class="title-text">تاريخ اصدار الفاتورة</div>

								<div class="title-value">{{ created_at }}</div>
							</td>
							<td class="title-container" style="direction: ltr;">
								<div class="title-text">Bill issue date</div>

								<div class="title-value">{{ created_at }}</div>
							</td>
									<td class="title-container" style="direction: ltr;">
								<div class="title-text">Payment method</div>
								<div class="title-value">Mada & Visa</div>
							</td>
						{% endif %}
						{% if payment_method == 'sadad' %}
							<td class="title-container">
								<div class="title-text">طريقة الدفع</div>
								<div class="title-value">سداد</div>
							</td>
							<td class="title-container">
								<div class="title-text">رقم الفاتورة</div>

								<div class="title-value">{{ sadad_number|arabicDigits }}</div>
							</td>
							<td class="title-container" style="direction: ltr;">
								<div class="title-text">SADAD number</div>
								<div class="title-value">{{ sadad_number }}</div>
							</td>
							<td class="title-container" style="direction: ltr;">
								<div class="title-text">Payment method</div>
								<div class="title-value">SADAD</div>
							</td>
							</tr>
							<tr>
							<td class="title-container">
								<div class="title-text">تاريخ انتهاء الفاتورة</div>

								<div class="title-value">{{ expired_at|hijiriDate|arabicDigits }}</div>
							</td>
								<td class="title-container">
								<div class="title-text">تاريخ اصدار الفاتورة</div>

								<div class="title-value">{{ created_at|hijiriDate|arabicDigits }}</div>
							</td>
							<td class="title-container" style="direction: ltr;">
								<div class="title-text">Bill issue date</div>

								<div class="title-value">{{ created_at }}</div>
							</td>
								<td class="title-container" style="direction: ltr;">
								<div class="title-text">Bill expiry date</div>

								<div class="title-value">{{ expired_at }}</div>
							</td>
						{% endif %}
					</tr>
				</tbody>
			</table>

			<div style="height: 50pt"></div>
			<table id="invoice" style="padding: 5pt 5pt; width: 100%; ">
				<thead>
					<tr style="background-color: #F3F3F3; color: black;     border: 2pt solid #F3F3F3;">
						<th class="table-header">
							<div>نوع الشراء / Purchase type</div>
						</th>
						<th class="table-header">
							<div>عدد التصاريح / Number of notices</div>
						</th>
						<th class="table-header">
							<div>السعر / Price</div>
						</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td class="p-10">
							 تصاريح / Notices
						</td>
						<td class="px-10">
							{{number_of_notices}}
						</td>
						<td class="px-10">
							<table style="width: 100%;">
								<tr>
									<td style="border: none; width: 100%;">{{notices_cost}}</td>
									<td style="border: none;">SAR</td>
								</tr>
							</table>
						</td>
					</tr>
					<tr>
						<td colspan="2" class="align-right px-10 py-5 font-bold text-small border-none">
							<div>المجموع الفرعي / Subtotal</div>
						</td>
						<td class="px-10 py-5">
							<table style="width: 100%;">
								<tr>
									<td style="border: none; width: 100%;">{{subtotal}}</td>
									<td style="border: none;">SAR</td>
								</tr>
							</table>
						</td>
					</tr>
					<tr>
						<td colspan="2" class="align-right px-10 py-5 font-bold text-small border-none">
							<div>الضريبة المضافة / Added tax</div>

						</td>
						<td class="px-10 py-5">
							<table style="width: 100%;">
								<tr>
									<td style="border: none; width: 100%;">{{added_tax}}</td>
									<td style="border: none;">SAR</td>
								</tr>
							</table>
					</tr>
					<tr>
						<td colspan="2" class="align-right px-10 font-bold text-small border-none">
							<div>مجموع / Total</div>
						</td>
						<td class="p-10 font-bold" style="background-color: #F3F3F3;">	<table style="width: 100%;" class="font-bold">
							<tr>
								<td style="border: none; width: 100%;">{{total}}</td>
								<td style="border: none;">SAR</td>
							</tr>
						</table></td>
					</tr>
				</tbody>
			</table>
			</div>
			<hr color="#152e83"></hr>
	</body>
</html>
